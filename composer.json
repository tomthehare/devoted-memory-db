{
    "require": {
        "php": ">=8.2",
        "psr/log": "^3.0",
        "psr/simple-cache": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Devoted\\MemoryDB\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Devoted\\MemoryDB\\Tests\\": "tests/"
        }
    },
    "config": {
        "vendor-dir": "/devoted/memory-db/vendor",
        "optimize-autoloader": true,
        "platform": {
            "php": "8.2.0"
        },
        "sort-packages": true
    }
}
