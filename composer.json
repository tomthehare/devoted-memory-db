{
    "require": {
        "php": ">=8.2",
        "bramus/monolog-colored-line-formatter": "^3.1",
        "monolog/monolog": "^3.5",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpspec/prophecy-phpunit": "^2.1",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Devoted\\MemoryDB\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Devoted\\MemoryDB\\Tests\\": "tests/"
        }
    },
    "config": {
        "vendor-dir": "/devoted/memory-db/vendor",
        "optimize-autoloader": true,
        "platform": {
            "php": "8.2.0"
        },
        "sort-packages": true
    }
}
